      <bind constraint="regex(., &quot;^[a-zA-Z0-9. _-]{1,9}$&quot;)" nodeset="/Hluvukani_B_registrar_pessoas/contacto" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/Hluvukani_B_registrar_pessoas/end" type="dateTime"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Hluvukani_B_registrar_pessoas/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat('Pessoas','_', /Hluvukani_B_registrar_pessoas/region_id ,'_', /Hluvukani_B_registrar_pessoas/bloco_id )" nodeset="/Hluvukani_B_registrar_pessoas/meta/instanceName" type="string"/>
