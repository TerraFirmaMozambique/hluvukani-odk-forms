      </item>
      <item>
        <label>Illovo/Maragra</label>
        <value>Illovo</value>
      </item>
    </select1>
    <select1 appearance="minimal" ref="/Hluvukani_registo_horas/bloco_id">
      <label>Selecionar o bloco</label>
      <itemset nodeset="instance('blocos')/root/item[region_id= /Hluvukani_registo_horas/region_id ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 appearance="minimal" ref="/Hluvukani_registo_horas/act">
      <label>Tipo de actividade</label>
      <item>
        <label>Sensibilização</label>
